{
	"HarvesterSettings":
	{
		"HarvesterSchedule": "*/5 * * * *",
		"ConfiguredSources": ["solar", "wod"],
		"KeyVaultUri": "https://krumelurvault.vault.azure.net"
	},

	"Logging": {
		"LogLevel": {
		  "Default": "Debug",
		  "System": "Information",
		  "Microsoft": "Error"
		},
		"File": {
			"Path": "harvester.log",
			"Append": "True",
			"FileSizeLimitBytes": 1048576,
			"MaxRollingFiles": 3 
		}
	}
}